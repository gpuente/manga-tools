{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "release/app/**"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "build:cli": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"]
    },
    "build:electron-app": {
      "dependsOn": ["build"],
      "outputs": ["release/build/**"]
    },
    "clean": {},
    "clean:full": {},
    "clean:cache": {},
    "clean:node_modules": {},
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
